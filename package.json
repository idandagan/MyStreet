{
  "name": "MyStreet",
  "version": "1.0.1",
  "scripts": {
    "build": "npm run clean && npm run build:prod && npm run build:server && npm run start",
    "build:server": "webpack --config webpack.config.babel.js webpack.config.server.js",
    "build:prod": "NODE_ENV=production webpack --config webpack.config.babel.js",
    "start:dev": "./node_modules/babel-cli/bin/babel-node.js servers.js",
    "start": "NODE_ENV=production webpack -p --config ./webpack.development.config.js",
    "start:dev": "node servers.js --exec babel-node --presets es2015,stage-2",
    "start-client": "nodemon ./client/dev-server.js --exec babel-node --presets es2015,stage-2",
    "start-server": "nodemon ./server/server.js --exec babel-node --presets es2015,stage-2",
    "clean": "rimraf dist"
  },
  "babel": {
    "presets": [
      "es2015",
      "react",
      "stage-2",
      "stage-0"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/idandagan1/MyStreet"
  },
  "author": "Idan Dagan and Stephen Lenrow",
  "license": "ISC",
  "engines": {
    "node": "6.9.4"
  },
  "dependencies": {
    "babel-cli": "^6.11.4",
    "babel-core": "^6.11.4",
    "babel-eslint": "^6.1.2",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.11.1",
    "babel-preset-stage-0": "^6.5.0",
    "babel-preset-stage-2": "^6.11.0",
    "body-parser": "^1.15.2",
    "clean-webpack-plugin": "^0.1.16",
    "connect-mongo": "^1.3.2",
    "css-loader": "^0.23.1",
    "eslint": "^3.2.2",
    "eslint-config-airbnb": "^10.0.0",
    "eslint-plugin-import": "^1.12.0",
    "eslint-plugin-jsx-a11y": "^2.0.1",
    "eslint-plugin-react": "^6.0.0",
    "express": "^4.14.0",
    "express-session": "^1.14.2",
    "express-validator": "^2.21.0",
    "file-loader": "^0.9.0",
    "html-loader": "^0.4.5",
    "html-webpack-plugin": "^2.22.0",
    "jquery": "^3.1.1",
    "jquery-ui": "^1.12.1",
    "moment": "^2.17.1",
    "mongoose": "^4.6.5",
    "morgan": "^1.7.0",
    "node-sass": "^3.8.0",
    "nodemon": "^1.11.0",
    "passport": "^0.3.2",
    "passport-facebook": "^2.1.1",
    "path": "^0.12.7",
    "react": "^15.3.0",
    "react-datepicker": "^0.53.0",
    "react-dom": "^15.3.0",
    "react-redux": "^4.4.5",
    "react-router": "^2.6.1",
    "react-router-redux": "^4.0.7",
    "redux": "^3.5.2",
    "redux-thunk": "^2.1.0",
    "sass-loader": "^4.0.0",
    "style-loader": "^0.13.1",
    "underscore": "^1.8.3",
    "url-loader": "^0.5.8",
    "webpack": "^1.13.1",
    "webpack-dev-server": "^1.14.1"
  },
  "devDependencies": {
    "css-loader": "^0.23.1",
    "react-input-range": "^1.2.1",
    "universal-cookie": "^2.1.0"
  }
}
